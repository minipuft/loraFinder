{
  "name": "lorafinder",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --config vite.config.client.ts --port 5173",
    "dev:server": "cross-env NODE_ENV=development tsx watch --ignore 'vite.config.*' --ignore '*.timestamp-*' --clear-screen=false server.ts",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "vite build --config vite.config.client.ts",
    "build:server": "vite build --config vite.config.server.ts",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rimraf dist",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "preinstall": "node ./scripts/check-engines.js",
    "postinstall": "npm run validate:deps",
    "validate:deps": "npm ls",
    "check:circular": "madge --circular --extensions ts,tsx ./src",
    "check:unused": "depcheck",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@tabler/icons-react": "^3.14.0",
    "axios": "^1.7.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "formidable": "^3.5.1",
    "framer-motion": "^11.5.4",
    "gsap": "^3.12.5",
    "idb": "^8.0.0",
    "image-size": "^1.1.1",
    "lottie-react": "^2.4.0",
    "node-cache": "^5.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^9.13.1",
    "react-masonry-css": "^1.0.16",
    "react-masonry-list": "^1.0.6",
    "react-photo-album": "^3.0.2",
    "react-responsive-masonry": "^2.3.0",
    "react-spring": "^9.7.4",
    "sharp": "^0.33.5",
    "uuid": "^10.0.0",
    "yet-another-react-lightbox": "^3.21.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/image-size": "^0.8.0",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-responsive-masonry": "^2.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "madge": "^6.1.0",
    "postcss": "^8.4.41",
    "postcss-scss": "^4.0.9",
    "rimraf": "^5.0.5",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.7.1",
    "typescript": "^5.0.4",
    "vite": "^5.4.12",
    "vite-plugin-checker": "^0.8.0",
    "vite-tsconfig-paths": "^4.2.1",
    "worker-loader": "^3.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
