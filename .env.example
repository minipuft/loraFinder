# Environment variables for MediaFlow

# Server Configuration
PORT=3000
MEDIA_LIBRARY_PATH=./media # Default path to your media library

# Client Configuration (can be overridden by server)
VITE_API_BASE_URL=http://localhost:3000/api

# Server-side LRU Cache Configuration
SERVER_LRU_MAX_ITEMS=500         # Max number of items in the server LRU cache
SERVER_LRU_TTL_MS=3600000        # Time-to-live for server LRU cache items in milliseconds (1 hour)